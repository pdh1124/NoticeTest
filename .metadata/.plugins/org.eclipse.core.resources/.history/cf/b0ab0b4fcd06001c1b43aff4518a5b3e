import com.spring.board.vo.UserVo;

public class userServiceImpl implements memberService {

	@Autowired
	UserDao memberDao; 
	
	@Override
	public int memberJoin(UserVo memberVo) throws Exception {
		// TODO Auto-generated method stub
		return memberDao.memberJoin(memberVo);E
	}

}
